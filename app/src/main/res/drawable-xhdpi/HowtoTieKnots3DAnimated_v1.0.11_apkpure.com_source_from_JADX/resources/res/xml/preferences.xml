<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="sound"
        android:summary="@string/soundSummary"
        android:title="@string/sound_title" />
    <ListPreference
        android:defaultValue="500"
        android:dialogTitle="@string/speed_dialogTitle"
        android:entries="@array/speedTitles"
        android:entryValues="@array/speedValues"
        android:key="speed"
        android:summary="@string/speedSummary"
        android:title="@string/speed_title" />
</PreferenceScreen>
